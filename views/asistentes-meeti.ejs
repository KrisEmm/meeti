<main class="contenedor asistentes">
 <h1><%= nombrePagina %> </h1>
 <% if (asistentes.length>0) { %>
 <ul class="listado-asistentes">
  <% asistentes.forEach(asistente=>{ %>
  <li>
   <div class="imagen">
    <% if (asistente.imagen) { %>
    <img src="/uploads/perfilt/<% asistente.imagen %>">
    <% } %>
   </div>
   <div class="texto">
    <%= asistente.nombre %>
   </div>
  </li>
  <% }) %>
 </ul>
 <% } else { %>
 <p style="text-align: center;">AÃºn no hay asistentes en este Meeti</p>
 <% } %>
 <a href="/meeti/<%= slug %>" class=' btn btn-rojo' style="display:block !important; width: 18rem; margin:0 auto;">
  Regresar</a>
</main>
